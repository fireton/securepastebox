{
  "DataProtection": {
    "ApplicationName": {
      "EnvVarName": "DATA_PROTECTION_APPLICATION_NAME",
      "DefaultValue": "SecurePasteBox"
    },
    "CacheKey": {
      "EnvVarName": "DATA_PROTECTION_CACHE_KEY",
      "DefaultValue": "DataProtection-Keys"
    }
  },
  
  "KeyStorage": {
    "Type": {
      "EnvVarName": "KEY_STORAGE_TYPE",
      "DefaultValue": "Memory"
    }
  },
  
  "Redis": {
    "Configuration": {
      "EnvVarName": "REDIS_CONFIGURATION",
      "DefaultValue": "localhost:6379"
    },
    "InstanceName": {
      "EnvVarName": "REDIS_INSTANCE_NAME",
      "DefaultValue": "SecurePasteBox:"
    }
  },

  "RateLimit": {
    "MinIntervalSeconds": {
      "EnvVarName": "RATE_LIMIT_MIN_INTERVAL_SECONDS",
      "DefaultValue": 5
    }
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "AllowedHosts": "*"
}
